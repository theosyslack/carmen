{
  "name": "@crescendo-collective/carmen",
  "version": "1.2.0",
  "description": "Carmen will sneak around a website, finding precious elements and returning them to you.",
  "license": "MIT",
  "engines": {
    "node": "10 - 13"
  },
  "bin": {
    "carmen": "./dist/carmen.js"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "dependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^10.12.16",
    "@types/puppeteer": "^1.11.1",
    "@types/ramda": "^0.25.48",
    "chalk": "^2.4.1",
    "commander": "^3.0.1",
    "concurrently": "^4.1.2",
    "delay": "^4.1.0",
    "glob": "^7.1.4",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-fuzzy-path": "^2.1.0",
    "lighthouse": "^5.2.0",
    "parcel": "^1.12.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-shebang": "^1.2.8",
    "puppeteer": "^1.10.0",
    "ramda": "^0.26.1",
    "resemblejs": "^3.2.2",
    "ts-node": "^7.0.1",
    "typescript": "~3.6.1"
  },
  "scripts": {
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "prebuild": "tsc",
    "prestart": "tsc",
    "start-cli": "parcel ./src/carmen.ts --target node",
    "start-module": "parcel ./src/index.ts --target node",
    "start": "concurrently npm:start-*",
    "build-cli": "parcel build ./src/carmen.ts --target node",
    "build-module": "parcel build ./src/index.ts --target node",
    "build": "concurrently npm:build-*",
    "test": "node dist/index.js follow ./tests/travel-plan.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/trillitech/carmen.git"
  },
  "author": "Crescendo Collective",
  "homepage": "https://bitbucket.org/trillitech/carmen#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "puppeeteer",
    "testing"
  ]
}
